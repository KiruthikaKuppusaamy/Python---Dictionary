# len() - Returns the number of items in the dictionary

my_dict = {1:'a',2:'b',3:'c',4:'d',5:'e'}
print ("Items in the Dictionary :", my_dict)
print ("Length of the Dictionary:", len(my_dict))

~~~~~~~~~~~~~~~~~~~~
Output
~~~~~~~~~~~~~~~~~~~~
Items in the Dictionary : {1: 'a', 2: 'b', 3: 'c', 4: 'd', 5: 'e'}
Length of the Dictionary: 5

__________________________________________________________________________________________________


# clear() – remove all the items in the Dictionary

my_dict = {'a': 1, 'b': 2}
my_dict.clear()
print(my_dict)

~~~~~~~~~~~~~~~~~~~~
Output
~~~~~~~~~~~~~~~~~~~~
{}

__________________________________________________________________________________________________

# copy() – Returns a shallow copy

original = {'a': 1, 'b': 2}
copied = original.copy()
print(copied)

~~~~~~~~~~~~~~~~~~~~
Output
~~~~~~~~~~~~~~~~~~~~
{'a': 1, 'b': 2}

__________________________________________________________________________________________________

# get() – get the value of the specified key in the Dictionary

my_dict = {'a': 1, 'b': 2}
print(my_dict.get('a'))    

~~~~~~~~~~~~~~~~~~~~
Output
~~~~~~~~~~~~~~~~~~~~
1

__________________________________________________________________________________________________

# items() – Returns list of (key, value) pairs

my_dict = {'a': 1, 'b': 2}
print(my_dict.items())

~~~~~~~~~~~~~~~~~~~~
Output
~~~~~~~~~~~~~~~~~~~~
dict_items([('a', 1), ('b', 2)])

__________________________________________________________________________________________________

# keys() – Returns list of keys

my_dict = {'a': 1, 'b': 2}
print(my_dict.keys())

~~~~~~~~~~~~~~~~~~~~
Output
~~~~~~~~~~~~~~~~~~~~
dict_keys(['a', 'b'])

__________________________________________________________________________________________________

# values() – Returns list of values

my_dict = {'a': 1, 'b': 2}
print(my_dict.values())

~~~~~~~~~~~~~~~~~~~~
Output
~~~~~~~~~~~~~~~~~~~~
dict_values([1, 2])

__________________________________________________________________________________________________

# pop() – Removes key and returns value

my_dict = {'a': 1, 'b': 2,'c':3,'d':4}
print(my_dict)
my_dict.pop('a')
print(my_dict)

~~~~~~~~~~~~~~~~~~~~
Output
~~~~~~~~~~~~~~~~~~~~
{'a': 1, 'b': 2, 'c': 3, 'd': 4}
{'b': 2, 'c': 3, 'd': 4}

__________________________________________________________________________________________________

# popitem() – Removes and returns last inserted key-value pair

my_dict = {'a': 1, 'b': 2,'c':3,'d':4}
print(my_dict)
my_dict.popitem()
print(my_dict)

~~~~~~~~~~~~~~~~~~~~
Output
~~~~~~~~~~~~~~~~~~~~
{'a': 1, 'b': 2, 'c': 3, 'd': 4}
{'a': 1, 'b': 2, 'c': 3}

__________________________________________________________________________________________________

# update() – Updates dictionary with another dictionary

my_dict = {'a': 1}
my_dict.update({'b': 2, 'c': 3})
print(my_dict)

~~~~~~~~~~~~~~~~~~~~
Output
~~~~~~~~~~~~~~~~~~~~
{'a': 1, 'b': 2, 'c': 3}

__________________________________________________________________________________________________
